<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="ghasemiarani.iman.android_wake_lock">

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application>
        <receiver
            android:name="ghasemiarani.iman.android_wake_lock.AndroidWakeLockBroadcastReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="${applicationId}.com.ayandap.WakeUp" />
            </intent-filter>
        </receiver>
    </application>
</manifest>
